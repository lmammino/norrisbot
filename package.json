{
  "name": "norrisbot",
  "version": "1.0.5",
  "description": "a Slack bot that kicks asses (roundhouse-kicks to be accurate...)",
  "keywords": [
    "Slack",
    "bot",
    "Chuck Norris",
    "jokes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lmammino/norrisbot"
  },
  "bugs": {
